begin model

begin parameters
  k_t 2
  # k1 1
  # k2 10
  # k3 100
end parameters

begin molecule types
  A()
  B()
  C()
  dummy()
end molecule types

begin species
  A() 1000
  B() 1000
  C() 1000
end species

begin observables
  Molecules A A()
  Molecules B B()
  Molecules C C()
  Molecules dum dummy()
end observables

begin functions
   # will use a file
  # k2 = TFUN("test2.dat") # will use a file
  mctr = dum/1e4
  k1 = TFUN(mctr,'test.dat')
  k2 = (TFUN(mctr, "test.dat")/1e1) # will use a file
  k3 = (TFUN(mctr, "test.dat")/k_t)
  # k3 = tctr # will use a file
end functions

begin reaction rules
  dummy: 0 -> dummy() 1e4
  Ad: A() -> 0 k1
  Bd: B() -> 0 k2
  Cd: C() -> 0 k3
end reaction rules

end model

# generate_network({overwrite=>1})
